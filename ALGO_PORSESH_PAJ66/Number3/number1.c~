#include <io.h>
unsigned char b;
unsigned char c;
unsigned char d;

void main(void)
{
DDRB=0x00;
DDRC=0xFF;
DDRD=0xFF;
while (1)
    {
    // Please write your application code here   
    b=PINB;
    c=b&0b00001111;
    d=(b&0b111100000)>>4;
    PORTC=c;
    PORTD=d;

    }
}
